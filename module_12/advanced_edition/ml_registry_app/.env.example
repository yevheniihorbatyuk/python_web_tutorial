# Application settings
APP_NAME="ML Model Registry"
APP_VERSION="1.0.0"
DEBUG=False

# Database configuration
DATABASE_URL=postgresql+asyncpg://ml_registry:dev_password_change_in_prod@postgres:5432/ml_registry_db
DATABASE_ECHO=False

# Redis configuration
REDIS_URL=redis://:dev_redis_password@redis:6379/0

# MinIO/S3 configuration
MINIO_ENDPOINT=minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_BUCKET_NAME=ml-models
MINIO_SECURE=False

# JWT authentication
JWT_SECRET_KEY=your-secret-key-change-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# Password hashing
PASSWORD_BCRYPT_ROUNDS=12
